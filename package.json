{
  "name": "message-media-test",
  "version": "1.0.0",
  "main": "./src/index.ts",
  "license": "MIT",
  "scripts": {
    "start": "webpack serve --hot --open --mode development",
    "build": "webpack --config webpack.config.js --mode production",
    "lint": "standard src/**/*.tsx src/**/*.ts src/**/*.jsx src/**/*.js",
    "test": "jest --coverage"
  },
  "standard": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setupEnzyme.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "babel-jest",
      "^.+\\.(jpg|png|gif|svg)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@giphy/js-fetch-api": "^2.1.0",
    "@giphy/react-components": "^2.1.3",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.7.1",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/redux-mock-store": "^1.0.2",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.0.2",
    "enzyme-to-json": "^3.6.1",
    "html-webpack-plugin": "^5.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "sass": "^1.32.7",
    "style-loader": "^2.0.0",
    "webpack": "^5.21.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-jest": "^26.6.3",
    "dotenv-webpack": "^6.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "node-sass": "^5.0.0",
    "sass-loader": "^11.0.1",
    "source-map-loader": "^2.0.0",
    "ts-jest": "^26.5.0",
    "ts-loader": "^8.0.15",
    "typescript": "^4.1.3"
  }
}
